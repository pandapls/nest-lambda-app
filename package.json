{
  "name": "nest-lambda-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:webpack": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "_comment_local": "=== 本地开发 ===",
    "dev:setup": "./scripts/setup-local-dev.sh setup",
    "dev": "./scripts/setup-local-dev.sh nest",
    "dev:sam": "./scripts/setup-local-dev.sh sam",
    "dev:db": "./scripts/setup-local-dev.sh db",
    "dev:status": "./scripts/setup-local-dev.sh status",
    "dev:stop": "./scripts/setup-local-dev.sh stop",
    "dev:reset": "./scripts/setup-local-dev.sh reset",
    "_comment_prisma": "=== Prisma操作 ===",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "setup:ssm": "./scripts/setup-ssm.sh",
    "deploy:lambda": "./scripts/deploy.sh",
    "deploy:local": "./scripts/setup-local-dev.sh",
    "analyze:bundle": "npm run build:webpack -- --analyze"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.835.0",
    "@fastify/aws-lambda": "^5.1.4",
    "@fastify/cors": "^10.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-fastify": "^11.1.3",
    "@prisma/client": "6.10.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/aws-lambda": "^8.10.149",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "6.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
